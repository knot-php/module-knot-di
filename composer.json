{
  "name": "knot-php/module-knot-di",
  "description": "knot-lib/di installer modules",
  "keywords": ["PHP","DI","Dependency Injection","Module","kNot Framework"],
  "homepage": "https://github.com/knot-php/module-knot-di",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "knot-lib/di": "~0.2",
    "knot-lib/module": "~0.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "knotphp\\module\\knotdi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "knotphp\\module\\knotdi\\test\\": "test/",
      "knotphp\\module\\knotdi\\test\\classes\\": "test/classes/"
    }
  }
}